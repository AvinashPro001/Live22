        <div class="info-form bank-remain-list">
            <div class="pad-lr-comman">
                <form id="formWithdrawal">
                    <ul class="password-upadte no-mar">
                        <li class="mar-btm-fifteen" id="displayAccountNumber">
                            <label class="fleft"><p class="big no-mar lang" key="withdrawal_account_number">Account Number</p></label>
                            <label class="fright bank-icon bank-name mendatory">
                                <input id="txt_withdrawalAccountNumber" onkeypress="return event.charCode >= 48 && event.charCode <= 57" name="txt_withdrawalAccountNumber" type="number" pattern="\d*" style="font-size:8px;" class="reg-form" placeholder="Enter Account Number">

<style>
    .loadingImage {
        text-align: center;
        position: fixed;
        left: 0;
        top: 0;
        width: 100%;
        height: 100%;
        z-index: 9999;
        background: center no-repeat #0f203a;
        opacity: .9;
        background-image: url(../images/loading.gif);
        background-size: 90px 100px;
    }
</style>
<script>
    //#region LoaderShow

    function LoaderShow() { $(".loadingImage").show(); }
    function LoaderHide() { $(".loadingImage").hide(); }

    //#endregion

    $(document).ready(function () {
        LoaderShow();
        let url_string = window.location;
        let url = new URL(url_string);
        let name = url.searchParams.get("gamename");
        let code = url.searchParams.get("gamecode");
        if (code != null) {
            switch (name) {
                case 'Pragmatic': return openPragmaticGame(code);
                case 'Playtech': return openPlaytechGame(code);
                case 'GamePlay': return openGamePlayGame(code);
            }
        }
        GameLoginMobile(name);
    });
</script>

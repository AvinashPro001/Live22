                <li><a id="gameplay-game" href="#gameplay" data-toggle="tab"><img src="~/images/carousel_logos/gameplay_bw.png" alt="logo" /></a></li>
            </ul>
            <div class="tab-content">
                <div id="playtech" class="tab-pane fade in active">
                    <div class="menu-list-area">
                        <ul class="nav nav-pills">
                            <li class="active"><a data-toggle="pill" href="#all">All</a></li>
                            <li><a data-toggle="pill" href="#hot">Hot</a></li>
                            <li><a data-toggle="pill" href="#new">New</a></li>
                            <li><a data-toggle="pill" href="#arcade">Arcade</a></li>
                            <li><a data-toggle="pill" href="#slot">slot</a></li>

<script>
    window.onhashchange = function () {
        OpenTabOnHashChange()
    }

    $(document).ready(function () {
        OpenTabOnHashChange();
    });

    function OpenTabOnHashChange() {
        if (window.location.hash) {
            var hash = window.location.hash.substring(1);
            if (hash == 'deposit') {
                document.getElementById("deposit_tab").click();
            } if (hash == 'withdraw') {
                document.getElementById("withdraw_tab").click();
            } if (hash == 'transfer') {
                document.getElementById("transfer_tab").click();
            }
        }
        else
            document.getElementById("profile_tab").click();
    }

    function openCity(evt, cityName, linknum) {
        var i, tabcontent, tablinks;
        tabcontent = document.getElementsByClassName("tabcontent");
        for (i = 0; i < tabcontent.length; i++) {
            tabcontent[i].style.display = "none";
        }
        tablinks = document.getElementsByClassName("tablinks");
        for (i = 0; i < tablinks.length; i++) {
            tablinks[i].className = tablinks[i].className.replace(" active", "");
        }
        if (linknum != null && linknum != undefined) {
            tablinks[linknum].className += " active";
        }
        document.getElementById(cityName).style.display = "block";
        evt.currentTarget.className += " active";
    }
</script>

@page
@{
    ViewData["Title"] = "Payment - Webet333 -🥇";
}
<script>var counter = 0;
    setInterval(function () {
        var url = localStorage.getItem("gameURL");
        if (url != null && url != "null") {
            localStorage.setItem("gameURL", null);
            window.location.href = url;
        }
        else {

            ++counter
            var IsWindowClose = localStorage.getItem("IsWindowClose");
            console.log(IsWindowClose);
            if (IsWindowClose)
                window.close(); 
            if (counter == 30)
                window.close();
        }
    }, 1000)</script>

<script>
    $(document).ready(function () {
        AllAnnouncementsCallAPI();
        CallDownloadLinkAPI();
        SetUsernamePassword();
        OpenTabOnHashChange();
    });

    setTimeout(function () {
        SetUsernamePassword();
    },5000)

    setInterval(function () {
        if (document.getElementById("Pussy888IOS").src == "") CallDownloadLinkAPI();
    }, 1000)

    window.onhashchange = OpenTabOnHashChange;

    function OpenTabOnHashChange() {
        SetUsernamePassword();
        if (window.location.hash) {
            var hash = window.location.hash.substring(1);
            if (hash == '918Download')
                document.getElementById("defaultOpen").click();
            if (hash == 'JokerDownload')
                document.getElementById("JokerDownloadOpen").click();
            if (hash == 'MegaDownload')
                document.getElementById("MegaDownloadOpen").click();
            if (hash == 'Pussy888Download')
                document.getElementById("PussyDownloadOpen").click();
        }
        else
            document.getElementById("defaultOpen").click();
    }

    function openCity(evt, cityName, linknum) {
        SetUsernamePassword();
        var i, tabcontent, tablinks;
        tabcontent = document.getElementsByClassName("tabcontent");
        for (i = 0; i < tabcontent.length; i++) {
            tabcontent[i].style.display = "none";
        }
        tablinks = document.getElementsByClassName("tablinks");
        for (i = 0; i < tablinks.length; i++) {
            tablinks[i].className = tablinks[i].className.replace(" active", "");
        }
        if (linknum != null && linknum != undefined) {
            tablinks[linknum].className += " active";
        }
        document.getElementById(cityName).style.display = "block";
        evt.currentTarget.className += " active";
    }
    document.getElementById("defaultOpen").click();
</script>
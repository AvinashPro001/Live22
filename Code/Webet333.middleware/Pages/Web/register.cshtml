@page
@{
    ViewData["Title"] = "Register - Webet333 -🥇";
    Layout = "~/Pages/Shared/_Layout.cshtml";
}
<div class="annunce-section-wrap">
    <div class="container">
        <div class="annunce-section">
            <p class="annunce-title">
                <i class="fas fa-bullhorn"></i>
            </p>
            <p class="annunce-text">
                <marquee class="announcment-font" id="announcements"></marquee>
            </p>
            <div class="clear-fix"></div>
        </div>
    </div>
</div>



<section class="article_sec_wrap">
    <div class="about_us_wrap">
        <div class="container">
            <div class="account-reg pad-comman-fifteen white-bg-tab-panel">
                <h4 class="register symbol mar-btm-ten"><span class="lang" key="register_account">REGISTER</span></h4>
                <div role="form" method="post">
                    <div class="row">
                        <div class="col-sm-6">
                            <div class="info-form">
                                <p class="big-text mar-btm-ten">
                                    <strong>
                                        <span class="lang" key="account_information">
                                            ACCOUNT INFORMATION
                                        </span>
                                    </strong>
                                </p>
                                <div class="row form-group ml0 mr0">
                                    <label class="user-title col-md-3 pl0">Username :</label>
                                    <label class="user mendatory col-md-9 pl0">
                                        <input type="text" class="reg-form" placeholder="" />
                                        <p class="star">*</p>
                                        <p class="note no-mar mar-btm-ten">
                                            <span class="lang" key="username_instruction">
                                                Username must contain a length of 7 charaters
                                            </span>
                                        </p>
                                    </label>
                                </div>

                                <div class="row form-group ml0 mr0">
                                    <label class="user-title col-md-3 pl0">Password :</label>
                                    <label class="lock mendatory col-md-9 pl0">
                                        <input type="password" class="reg-form" placeholder="" />
                                        <p class="star">*</p>
                                        <p class="note no-mar mar-btm-ten">
                                            <span class="lang"> - Password must contain a length of 6 charaters</span>
                                        </p>
                                        <p class="note no-mar mar-btm-ten">
                                            <span class="lang"> - Password must be alphanumeric.</span>
                                        </p>
                                        <p class="note no-mar mar-btm-ten">
                                            <span class="lang"> - Password and username must be different.</span>
                                        </p>
                                    </label>
                                </div>

                                <div class="row form-group ml0 mr0">
                                    <label class="user-title col-md-3 pl0">Confirm Password :</label>
                                    <label class="lock mendatory col-md-9 pl0">
                                        <input type="password" class="reg-form" placeholder="">
                                        <p class="star">*</p>
                                    </label>
                                </div>

                                <p class="big-text no-mar">
                                    <strong>
                                        <span class="lang" key="contact_details">
                                            CONTACT DETAILS
                                        </span>
                                    </strong>
                                </p>
                                <p class="note no-mar mar-btm-ten">
                                    <span class="lang" key="contact_instruction">
                                        Please enter valid phone number to received TAC number for activation account
                                    </span>
                                </p>
                                <div class="row form-group ml0 mr0">
                                    <label class="user-title col-md-3 pl0">Phone Number :</label>
                                    <label class="num mendatory col-md-9 pl0">
                                        <input type="number" p class="reg-form" placeholder="" />
                                        <p class="star">*</p>
                                    </label>
                                </div>
                            </div>
                        </div>

                        <div class=" col-sm-6">
                            <div class="info-form">
                                <p class="big-text mar-btm-ten">
                                    <strong><span class="lang">BASIC INFORMATION</span></strong>
                                </p>
                                <div class="row form-group ml0 mr0">
                                    <label class="user-title col-md-3 pl0">Full Name as NRIC</label>
                                    <label class="mar-btm-no user mendatory col-md-9 pl0">
                                        <input type="text" class="reg-form" placeholder="" value="" />
                                        <p class="star">*</p>
                                    </label>
                                </div>

                                <p class="note no-mar lang">Registered name must match your bank account name for banking purpose.You wont able to change your name after registered</p>

                                <div class="row important_notice_register mr0 ml0">
                                    <!-- <div class="col-lg-2">&nbsp;</div> -->
                                    <div class="col-lg-10">
                                        <div class="info-form register-notice deposit-section pad-top-twentyfive" style="background-color: #fff; border-radius: 6px;">
                                            <div class="pad-lr-comman">
                                                <h6 class="info-notice">Information Notice</h6>
                                                <ol class="register-notes-list">
                                                    <li>Phone number is to retrieve your password when you are forgotten.</li>
                                                    <li>Any New rebate credit to your account, will get a SMS notification</li>
                                                    <li>Phone number is your identity, it is very important. please write a phone number you can receive SMS</li>
                                                </ol>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                            </div>
                        </div>
                    </div>
                    <hr>
                    <div class="row ml0 mr0">
                        <div class="col-md-4 recaptcha" id="g-recaptcha1"></div>
                    </div>
                    <div class="checkbox">
                        <label class="tick-note accept_terms_wrap">
                            <input type="checkbox" value="0" class="accept_terms">
                            <span class="accept_terms">I accept the Terms & Conditions , Privacy Policy and Age Verification Policy as published on this site.</span>
                        </label>
                    </div>
                    <div class="text-left mar-comman">
                        <button type="button" value="Submit" class="submit">
                            <span>SUBMIT</span>
                        </button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

<script src='https://www.google.com/recaptcha/api.js?onload=onloadCallback'></script>
<script type="text/javascript">
    AllAnnouncementsCallAPI();
        // setTimout is not necessary
          setTimeout(function() {

            $('.recaptcha').each(function() {
              grecaptcha.render(this.id, {
                'sitekey': '6LdVkwkUAAAAACeeETRX--v9Js0vWyjQOTIZxxeB',
                "theme":"light"
              });
            });

          }, 1000);
</script>
